
class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int temp;
        int ele=0;
        int i;
        
        for(i=0;i<arr.length;i++)
        {
            if(arr[i]!=0)
                {
                    temp=arr[i];
                    arr[i]=arr[ele];
                    arr[ele]=temp;
                    ele++;
                }
        }        
        
    }
}
